all:
  hosts:
    gto-solver:
      ansible_host: YOUR_VM_IP_ADDRESS  # Replace with your VM IP (e.g., 192.168.1.100)
      ansible_user: ansible
      ansible_password: YOUR_ANSIBLE_PASSWORD  # Replace with the password you set
      ansible_connection: winrm
      ansible_winrm_transport: basic
      ansible_winrm_server_cert_validation: ignore
      ansible_port: 5985

  vars:
    gto_service_port: 8080
    python_version: "3.11.0"
